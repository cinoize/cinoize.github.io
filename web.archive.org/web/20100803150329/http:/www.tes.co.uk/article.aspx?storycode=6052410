<script type="text/javascript">


$(function()	{
$('#my_account:not(.loggedout)>ul>li>a').click(showHideMyAccountBox); 

function showHideMyAccountBox() {
    
    // Find out if we're logged out
	var isLoginOrRegister =  (this.parentNode.id == 'my_account_login') ||  (this.parentNode.id == 'my_account_register');	
    
    
    // register panel has different properties to the rest
    var isRegisterPanel = (this.parentNode.id == 'my_account_register');
    // set up the required height
    var openHeight = isRegisterPanel ? 500 : 177;
    // Save a pointer to the event
    var pointer = this;
	
    // Check to see if the tab has been loaded
    if(!isLoginOrRegister && !$(this).parent().hasClass('loaded'))	{
	    // It hasn't. Set the innerHTML to be the loading screen
	    $(this).next().find('.sleeve').html('<div class="AJAX_load"></div>');
	    // New AJAX request
	    $.post(
		    'AJAX/MyAccount.ajax.aspx',
		    { 
			    // Here we can post data to the server
			    type: $(this).parent().attr('id').replace("my_account_", "")
		    },
		    function(reply)	{
			    // Update the correct area
			    $(pointer).next().find('.sleeve').html(reply);
			    // Add a class to say we've loaded the tab
			    $(pointer).parent().addClass('loaded');
			    //Check if comming from jobs then add valdiation
			    if ($(pointer).parent().find('a.tab').html() == 'Jobs')
			    {
			        //addJobsAlertsValidation();
			    }
		    },
		    "html"
	    );
    }
	
	
    // if already open close it
    if($(this.parentNode).is('.open')) {
	    $(this).next().animate({height:'0'},{duration:400,queue:false}).parent().animate({borderBottomWidth:'4px'},{duration:400}).removeClass('open');
    }
    // otherwise, open it
    else {
	    // if the open panel is next to the new panel then flick directly
	    if(($(this.parentNode).is(':nth-child(even)') && $(this.parentNode).prev('li').is('.open')) || ($(this.parentNode).is(':nth-child(odd)') && $(this.parentNode).next('li').is('.open')) ) {
		    $(this.parentNode).addClass('open').css('borderBottomWidth',openHeight+7+'px').children('div').css('height',openHeight+'px').end()
			    .siblings('.open').removeClass('open').css('borderBottomWidth','4px').children('div').css('height','0');
	    }
	    // otherwise animate between the two
	    else {
		    $(this.parentNode).addClass('open').animate({borderBottomWidth:openHeight+7+'px'},{duration:400,queue:false}).children('div').animate({height:openHeight+'px'},{duration:400,queue:false}).end()
			    .siblings('.open').removeClass('open').animate({borderBottomWidth:'4px'},{duration:400,queue:false}).children('div:visible').animate({height:'0'},{duration:400,queue:false});
				
	    }

	    if($(this).parents('#my_account').is('.loggedout')) { 
		    $('+div>div>iframe', this).height(openHeight+'px'); 

	    }
    }

	if($(this).next().is('.my_account_register_content'))	{
		/*try	{
			resizeRegisterPanel();
		} catch(e)	{}*/
	}
	if($(this).next().is('.my_account_login_content'))	{
		/*try	{
			resizeLoginPanel();
		} catch(e)	{}*/
	}
	return false;
}
});


</script>



					
