    $(document).ready(function() {
        $('#mast_search_form').submit(function()	{
            var keywords =  $('#mast_search').val();
            keywords = keywords.replace(/\&/gi,"%26").replace(/ /gi,"+");
			if ($("#search_type").val() == "rest")	
		
                       
                       //window.location ='searchResults.aspx?area=all&keywords=' + keywords;                      
                        window.location = window.location.protocol + '//' + window.location.host + '/'	+
                        'searchResults.aspx?area=all&keywords=' + keywords;  		
			else
//				window.location ='jobsSearchResults.aspx?keywords=' + keywords;
             window.location = window.location.protocol + '//' + window.location.host + '/'	+
             'jobsSearchResults.aspx?keywords=' + keywords;
            return false;
        });
          $("#mast_search").keydown(function(e) {            
                if (e.keyCode == 13)	{
                        var keywords =  $('#mast_search').val();
                        keywords = keywords.replace(/\&/gi,"%26").replace(/ /gi,"+");
                        if ($("#search_type").val() == "rest")	
//							window.location ='searchResults.aspx?area=all&keywords=' + keywords;
							 window.location = window.location.protocol + '//' + window.location.host + '/'	+
                        'searchResults.aspx?area=all&keywords=' + keywords; 
                        else
//							window.location ='jobsSearchResults.aspx?keywords=' + keywords;
                            window.location = window.location.protocol + '//' + window.location.host + '/'	+
                                     'jobsSearchResults.aspx?keywords=' + keywords;
						$('#mast_search_form').submit();
                }
            });
    });
    
