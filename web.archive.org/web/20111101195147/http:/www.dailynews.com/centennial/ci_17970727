 }
  if (MNGiRegistrationLoginStatus == "in") {
      document.write("Welcome:&nbsp;" + MNGiRegistrationUserName + "&nbsp;&nbsp;&nbsp;<a class='pageLink' href="https://web.archive.org/web/20111101195147/http://www.dailynews.com/centennial/+&#32;MNGiRegistrationLoginUrl&#32;+"?rEmail=" + MNGiRegistrationEmail + ">Manage Profile</a>&nbsp;&nbsp;|&nbsp;&nbsp;<a class='pageLink' href='https://web.archive.org/web/20111101195147/http://www.dailynews.com/centennial/&quot;&#32;+&#32;MNGiRegistrationUrl&#32;+&#32;&quot;/portlet/registration/html/logout.jsp'>Logout</a>");
  }
                 else {
      document.write("<a class='pageLink' href="https://web.archive.org/web/20111101195147/http://www.dailynews.com/centennial/+&#32;MNGiRegistrationLoginUrl&#32;+">Login</a> | <a class='pageLink' href='https://web.archive.org/web/20111101195147/http://www.dailynews.com/centennial/&quot;&#32;+&#32;MNGiRegistrationUrl&#32;+&#32;&quot;/registration?Page=register'>Newsletter</a>");
                 }
 </script>

 <!--END REGISTRATION-->
    
    </li>
    
