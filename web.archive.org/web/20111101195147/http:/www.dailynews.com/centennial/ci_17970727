		<div id="articleOverline" class="articleOverline">DAILY NEWS CENTENNIAL</div>
    


<!--title-->

    
    

    
    
		<h1 id="articleTitle" class="articleTitle">Political activism grows</h1>
    


<!--subtitle-->

    
    


<!--byline-->
<div id="articleByline" class="articleByline">
	
	    
	    
	    
	    
	    
	
	    
	    
	    
	    
	        By Andrea Hescheles, Correspondent
	    
	    
	
</div>

<!--date-->

    
    

    
    
		<div id="articleDate" class="articleDate">Posted:&nbsp;05/01/2011 06:12:14 PM PDT</div>
	


<!--secondary date-->


	
	

	
	
		
			
			
				
		
			
			
				
