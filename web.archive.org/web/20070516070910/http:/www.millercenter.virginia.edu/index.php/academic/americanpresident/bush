	<script src="https://web.archive.org/web/20070516070910js_/http://www.millercenter.virginia.edu/dev/ci/system/application/views/jscript/mediahelp.js"></script>
	<script language="Javascript">
		function setOverTimer(tab) {
			if(lastTab != tab) {
				if(overTimer != undefined)
					window.clearTimeout(overTimer);
				overTimer = window.setTimeout(function(){changeMenu(tab);}, 90);
			}
		}
	
		function changeMenu(tab) {
			for(var i = 0; i < 8; i++) {
				document.getElementById('menutab'+i).style.display = 'none';
			}
			document.getElementById('menutab'+tab).style.display = 'block';
			changeSubMenu(tab);
			lastTab = tab;
		}
		
		function changeSubMenu(tab) {
			str = '';
			for(i = 0; i < submenu[tab].length; i++) {
				if(submenulink[tab][i] != 'NOLINK')
					str += '<a href="https://web.archive.org/web/20070516070910/http://www.millercenter.virginia.edu/index.php/academic/americanpresident/'&#32;+&#32;submenulink[tab][i]&#32;+&#32;'?PHPSESSID=44f8585805a1faac44434d51a26d42f5">' + submenu[tab][i] + '</a>';
				else
					str += submenu[tab][i];
				
				if(i != submenu[tab].length-1)
					str += " | ";
			}
			document.getElementById('submenu').innerHTML = str;
		}
		
		function setOutTimer() {
			if(lastTab != selectedTab) {
				if(outTimer != undefined)
					window.clearTimeout(outTimer);
				outTimer = window.setTimeout(function(){changeMenu(selectedTab)}, 3200);
			}
		}
		
		var selectedTab = 3;
		var lastTab = selectedTab;
		var overTimer;
		var outTimer;
		changeMenu(selectedTab);
	</script>
