                      {{/each}}
                    {{/if}}
                    
                    {{#if propertiesArray}}
                      {{#each propertiesArray}}
                        <li>
                            {{ key }}:
                          {{ value }}
                        </li>
                      {{/each}}
                    {{/if}}

                    <li>{{ variant_title }}</li>
                    <li>Qty: {{ quantity }}</li>
                    <li>Price: {{#if compare_at_price }}<s>{{ compare_at_price }}</s>{{/if}} <span class="{{#if compare_at_price }}AjaxCart-sale-price{{/if}}">{{ price }}</span></li>
                    <li>
                      <a href="#" class="AjaxCart-itemRemove" data-ajax-cart-item-remove>
                        <span class="u-pos-rel" style="top: -1px;">&times;</span>
                        Remove
                      </a>
                    </li>
                  </ul>
                </div>
              </li>
              {{/each}}
              
            </ul>
          </div>
        {{else}}
          <div class="AjaxCart__empty-message p1">
            
            
              <p>Your bag is empty</p>
            
            
          </div>
        {{/if}}
      </div>
      
      
      
      {{#if items}}
        <div class="AjaxCart-footer" data-ajax-cart-footer>
         
          <p class="p3 AjaxCart-footer__free-shipping-calculator " id="freeShippingCalculator"><span>
             
            
            
              
            
          </span></p>


          
          
      

          
          <div class="AjaxCart-footer__row">
            <p class="h5 u-ta-right"><span style="display: inline-block; margin-right: 8px;">Total</span> {{ total_price }}</p>  
          </div>
          <div class="AjaxCart-footer__row">
            <a href="/cart" class="btn btn-primary btn-block" data-ajax-cart-cart-page-link>View Bag</a>
            <a href="/cart" class="btn btn-primary btn-block cart-continue-shopping" data-ajax-cart-close>Continue Shopping</a>
            <a class="btn btn-primary btn-block cart-back-to-surf-ranch" href="/pages/surf-ranch-collection-mens">Back To Surf Ranch Shop</a>
            <!-- <button type="submit" name="checkout" class="btn btn-primary btn-block">View Bag</button> -->
          </div>
          
          
            <div class="AjaxCart-footer__row u-ta-center">
              <p>FREE U.S. SHIPPING ON ORDERS OVER $100</p>
<p>FREE INTERNATIONAL SHIPPING ON ORDERS OVER $200 USD</p>
            </div>
          
          
        </div>
      {{/if}}
    </form>
  </div>
</script>




 <!-- Begin Shopify-Afterpay JavaScript Snippet (v1.0.1) -->
  <script type="text/javascript">
    
    const CART_CLASSNAME = 'AjaxCart';
    var apCartConfig = {
      priceSelector: '#shopify-section-ajax-cart > div > div > div > form > div.AjaxCart-footer > div:nth-child(2) > p',
      locale: 'en_US',
      amount: 0,
      currency: 'USD',
      minMaxThreshold: {
        min: 3500,
        max: 100000,
      },
    };

    let cartContainerObserver;
    let cartContainer = document.querySelector('.AjaxCart-container');
    let config = { childList: true, subtree: true };
    let cartContainerCallback = (mutationsList, observer) => {
      for (let mutation of mutationsList) {
        for (let node of mutation.addedNodes) {
          if (node.className && node.className === CART_CLASSNAME) {

            let productTitles = Array.prototype.slice.call(node.querySelectorAll('.ajaxcart__product .grid__item.two-thirds'));
            let numberOfGiftCards = productTitles.filter(product => product.innerText.toLowerCase().includes('gift card')).length;
            if (numberOfGiftCards === 0) {
              let formattedPrice = node.querySelector('.AjaxCart-footer .AjaxCart-footer__row').innerText;
              formattedPrice = formattedPrice.replace(/[^0-9]/g, '');
              apCartConfig.amount = Number(formattedPrice.replace(/[^a-zA-Z0-9 ]/g, ''));
              if (node.querySelector('p.afterpay-paragraph')) {
                node.querySelector('p.afterpay-paragraph').remove();
              }
              new presentAfterpay(apCartConfig).init();
            }
          }
        }
      }
    } 

    cartContainerObserver = new MutationObserver(cartContainerCallback);
    cartContainerObserver.observe(cartContainer, config);
    cartContainerObserver.disconnect();
        
  </script>
  <!-- End Shopify-Afterpay JavaScript Snippet (v1.0.1) -->


</div>
 
  

  

    
