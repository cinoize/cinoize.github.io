        <script>
          var ftrSiteName = [, "Esquire"];
          var uniq_ftrSiteName = ftrSiteName.reduce(function(a,b){if (a.indexOf(b) < 0 ) a.push(b); return a;},[]); //remove dupes
          var str_ftrSiteName;
          if (uniq_ftrSiteName.length == 1) {
            str_ftrSiteName = uniq_ftrSiteName[0];
          } else {
            if (uniq_ftrSiteName.length == 2) {
              str_ftrSiteName = uniq_ftrSiteName.join(' and ');
            } else {
              if (uniq_ftrSiteName.length > 2) {
                str_ftrSiteName = [uniq_ftrSiteName.slice(0, -1).join(', '), uniq_ftrSiteName.slice(-1)[0]].join(uniq_ftrSiteName.length < 2 ? '' : ', and ');
              }
            }
          }
          var ftrSiteCopy = [, "<i>Esquire</i>&apos;s cover price is $6.99 an issue and is published 6 times a year, Mar, Apr/May, Summer, Sept, Oct/Nov, Winter and when future combined issues are published that count as two issues as indicated on the issue&apos;s cover."];
          var uniq_ftrSiteCopy = ftrSiteCopy.reduce(function(a,b){if (a.indexOf(b) < 0 ) a.push(b); return a;},[]); //remove dupes
          var str_ftrSiteCopy;
          if (uniq_ftrSiteCopy.length == 1) {
            str_ftrSiteCopy = uniq_ftrSiteCopy[0];
          } else {
            if (uniq_ftrSiteCopy.length >= 2) {
              str_ftrSiteCopy = uniq_ftrSiteCopy.join(' ');
            }
          }
          document.getElementById('ftrSiteName').innerHTML = str_ftrSiteName;
          if (str_ftrSiteCopy != null) {
            document.getElementById('ftrSiteCopy').innerHTML = str_ftrSiteCopy;
          }
        </script>

