  <script language="Javascript" type="text/javascript">
var HEARST_circ = {
   "page" : "landing",
   "up" : 0,
   "campaignId" : "234893",
   "campaignName" : "ESQ_DIG_Gift_6/$15_SingleGift_Mar18",
   "pagename" : "https://subscribe.hearstmags.com/subscribe/landing",
   "cc" : 0,
   "site" : "Esquire",
   "keycode" : "",
   "urId" : "",
   "type" : "gift",
   "transId" : null,
   "products" : ";self:: months,;self:Esquire:12 months"
}
;
</script>

  <script type="text/javascript" src="/circulation/shared/scripts/jquery-latest-circ.min.js"></script>
  <script type="text/javascript" src="/circulation/shared/scripts/jquery.validate.1.6.0.min.js"></script>
  <script type="text/javascript" src="/circulation/shared/scripts/additional-methods.min.js"></script>


  <script type="text/javascript">
    var isBillme = true,
      isGift = false,
      isIntl = false,
      isIntl1 = false,
      isDigital = false,
      payment_id = '';

    function beforeSubmit() {
      $('#btn_submit').hide();
      $('#btn_processing').show();
    }

    // post-submit callback
    function showResponse(res, statusText, xhr, $form)  {
      var $error = $("#error");
      var response = eval('(' + res + ')');

      if(response.order_success[0] == ""){
        response.order_failed[0] = filterBrackets(response.order_failed[0]);

        if (response.order_failed[0] == "You are not eligible to subscribe to this service.") {
          $error.html("<p>You do not meet the age requirement to order.</p>");
        } else {
          $error.html("<p>"+response.order_failed[0]+"</p>");
        }
        $error.show();
        $("html,body").animate({'scrollTop': $error.offset().top-10}, 100);
        $("#btn_processing").hide();
        $("#btn_submit").show();

      } else {
        var decoded = $(document.createElement("div")).html(response.order_success[0].next_offer_url[0]).text();
        window.location = decoded;
        //window.location = response.order_success[0].next_offer_url[0];
      }

    }

    function filterBrackets(r) {
      var returnVal = r.replace(/\[/gi, '<');
      returnVal = returnVal.replace(/\]/gi, '>');
      return returnVal;
    }

    var win_w = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;

    $(function(){

      $(window).resize(function() {
        win_w = $(window).width();
      });

      /* CVV POPUP */
      var isiDevice = /ipad|iphone|ipod/i.test(navigator.userAgent.toLowerCase());
      var cvv_help = $("#cvv .help");

      if (!isiDevice) {
        $("#cvv").mouseenter(function(){
          cvv_help.show();
        }).mouseleave(function() {
          cvv_help.hide();
        });
      } else {
        $("#cvv").click(function(){
          cvv_help.toggle();
        });
      }

       /***** DEFAULT VALIDATION *****/

      $('#pt_id').change(function() {
        payment_id = $('#pt_id option:selected').val();
        reqCC(payment_id);
      });

      //CHECKS IF BILL ME ALREADY EXISTS
      if ($('#pt_id option[value="6"]').length == 0) {
        isBillme = false;
      }

      //CHECKS IF CURRENT OFFER IS DIGITAL AND IF YES REMOVE BILLME
      if (($('#offerterms input:checked').attr('alt') == 'B')||($('#offerterms input:checked').attr('alt') == 'D')) {
        isDigital = true;
        $("#pt_id option[value='6']").remove();
      }

      //CHECK IF CANADIAN/INTERNATIONAL AND REMOVE STATE REQUIREMENT
      checkIsIntl();

      //CHECK CANADIAN/INTERNATIONAL ORDERS AND SHOW SHIPPING MESSAGES
      $('.country_code, .ship_country_code').change(function() {
        checkIsIntl();
      });

      function checkIsIntl() {
        var checkIntl = $('.country_code option:selected').val(),
            checkIntl1 = $('.ship_country_code option:selected').val(),
            checkIntl2 = $('.g2_ship_country_code option:selected').val();

        //check intl for gift donor
        if ((typeof checkIntl != 'undefined') && (checkIntl != '') && (checkIntl != 'US')) {
          if (checkIntl == 'CA') {
            isIntl = true;
            $('.int_ship').hide();
            $('.can_ship').show();
            $("#pt_id option[value='6']").remove();
            //if ($('#pt_id option[value="6"]').length == 0 && isBillme && !isIntl && !isGift && !isDigital) {
            //  $('#pt_id').append($('<option></option>').val('6').html('Bill Me Later'));
            //}
            $('.state').attr('required','required');
          } else {
            isIntl = true;
            $('.can_ship').hide();
            $('.int_ship').show();
            $("#pt_id option[value='6']").remove();
            $('.state').removeAttr('required');
          }
        } else {
          isIntl = false;
          $('.can_ship, .int_ship').hide();
          if ($('#pt_id option[value="6"]').length == 0 && isBillme && !isIntl && !isIntl1 && !isGift && !isDigital) {
            $('#pt_id').append($('<option></option>').val('6').html('Bill Me Later'));
          }
          $('.state').attr('required','required');
        }


        //check intl for gift recip 1
        if ((typeof checkIntl1 != 'undefined') && (checkIntl1 != '') && (checkIntl1 != 'US')) {
          if (checkIntl1 == 'CA') {
            isIntl1 = true;
            $("#pt_id option[value='6']").remove();
            //if ($('#pt_id option[value="6"]').length == 0 && isBillme && !isIntl && !isGift && !isDigital) {
            //  $('#pt_id').append($('<option></option>').val('6').html('Bill Me Later'));
            //}
            $('.ship_state.g1').attr('required','required');
          } else {
            isIntl1 = true;
            $("#pt_id option[value='6']").remove();
            $('.ship_state.g1').removeAttr('required');
          }
        } else {
          isIntl1 = false;
          if ($('#pt_id option[value="6"]').length == 0 && isBillme && !isIntl && !isIntl1 && !isGift && !isDigital) {
            $('#pt_id').append($('<option></option>').val('6').html('Bill Me Later'));
          }
          $('.ship_state.g1').attr('required','required');
        }
      }

      // REMOVE BILLME FOR DIGITAL ORDERS
      $('#offerterms input').click(function() {
        var checkdigital = $('#offerterms input:checked').attr('alt');
        if ((checkdigital == 'B')||(checkdigital == 'D')) {
          isDigital = true;
          $("#pt_id option[value='6']").remove();
        } else {
          isDigital = false;
          if ($('#pt_id option[value="6"]').length == 0 && isBillme && !isIntl && !isGift && !isDigital) {
            $('#pt_id').append($('<option></option>').val('6').html('Bill Me Later'));
          }
        }
      });


      var $cc_num = $("input[name='credit_card_number']"),
          $cc_type = $("input[name='pt_id']"),
          reg_amex = new RegExp("^(3)$"),
          reg_visa = new RegExp("^(4)$"),
          reg_mc = new RegExp("^(5|2)$"),
          reg_disc = new RegExp("^(6)$");

      $cc_num.keyup(function() {
        var cc_iin = $cc_num.val().slice(0,1);
        if (reg_amex.test(cc_iin)) {
          $cc_type.val('3');
        } else {
          if (reg_visa.test(cc_iin)) {
            $cc_type.val('2');
          } else {
            if (reg_mc.test(cc_iin)) {
              $cc_type.val('1');
            } else {
              if (reg_disc.test(cc_iin)) {
                $cc_type.val('5');
              }
            }
          }
        }
      });

      /***********************************/

      function reqCC(pt_id) {
        if (pt_id < 6) {
          $('input[name=credit_card_number],input[name=credit_card_exp_month],input[name=credit_card_exp_year],input[name=credit_card_cvv]').prop("required", true);
        } else {
          $('input[name=credit_card_number],input[name=credit_card_exp_month],input[name=credit_card_exp_year],input[name=credit_card_cvv]').prop("required", false);
        }
      }

      $('select').focus(function(){
          $(this).parent('.select').siblings('.select_bdr').css('border-color','#00848a');
        }).blur(function(){
          $(this).parent('.select').siblings('.select_bdr').css('border-color','#dbdbdb');
        });

      $('#frm2').validate();
      $('#frm3').validate();
      $('#frm4').validate({
        rules: {
          credit_card_number: {
            creditcard: true
          }
        },
        submitHandler: function(form) {
           beforeSubmit();

          if ($('input[name=chooseoffer]:checked').val() == '2gifts') {
            $('#g2 :input').prop('disabled', false);
            $('#g2 input[name=ship_first_name]').val( $('input[name=g2_ship_first_name]').val() );
            $('#g2 input[name=ship_last_name]').val( $('input[name=g2_ship_last_name]').val() );
            $('#g2 input[name=ship_address]').val( $('input[name=g2_ship_address]').val() );
            $('#g2 input[name=ship_address2]').val( $('input[name=g2_ship_address2]').val() );
            $('#g2 input[name=ship_city]').val( $('input[name=g2_ship_city]').val() );
            $('#g2 input[name=ship_state]').val( $('select[name=g2_ship_state]').val() );
            $('#g2 input[name=ship_postal_code]').val( $('input[name=g2_ship_postal_code]').val() );
            $('#g2 input[name=ship_country_code]').val( $('select[name=g2_ship_country_code]').val() );
          }
          var subForms = $('#frm_control, #frm1, #frm2, #frm3, #frm4');
          $.ajax({
            type: "POST",
            url: "/api/circ/js/Transaction",
            data: subForms.serialize(),
            success: function(response) {
              showResponse(response);
            }
          });
        }
      });

      $('select').focus(function(){
        $(this).parent('.select').siblings('.select_bdr').css('border-color','#000');
      }).blur(function(){
        $(this).parent('.select').siblings('.select_bdr').css('border-color','#ccc');
      });



      var $plus = $('.plus'),
          $button = $('.btn'),
          $chooseoffer = $('input[name=chooseoffer]'),
          $gift1 = $('#gift1 h3'),
          $gift2 = $('#gift2'),
          $gift2_input = $('#gift2 :input'),
          $offer = $('.offer .inner'),
          $gift = $('.gift .inner'),
          $self = $('.self .inner'),
          $payment = $('.payment .inner'),
          $offers = $("#offers input");

      $plus.click(function(){
        var $t = $(this);
        $('.inner').hide();
        $('h2').removeClass('active ESQ custom_color');
        $t.siblings('.inner').show();
        $t.siblings('h2').addClass('active ESQ custom_color');
      });


      $button.click(function(){
        var $t = $(this);

        if ($t.hasClass('goto_gift')) {
          $offer.siblings('.plus').show();
          $offer.siblings('h2').removeClass('active ESQ custom_color');
          $offer.hide();
          $gift.siblings('h2').addClass('active ESQ custom_color');
          $gift.show();
          if (win_w < 480) {
            $('html,body').animate({'scrollTop':  $gift.offset().top-50}, 200);
          } else {
            $('html,body').animate({'scrollTop':  $gift.offset().top-65}, 200);
          }
        } else if ($t.hasClass('goto_self')) {
          if ($('#frm2').valid()) {
            $gift.siblings('.plus').show();
            $gift.siblings('h2').removeClass('active ESQ custom_color');
            $offer.closest('.offer').addClass('addborder');
            $gift.hide();
            $self.siblings('h2').addClass('active ESQ custom_color');
            $self.show();
            if (win_w < 480) {
              $('html,body').animate({'scrollTop':  $self.offset().top-50}, 200);
            } else {
              $('html,body').animate({'scrollTop':  $self.offset().top-65}, 200);
            }
          }
        } else if ($t.hasClass('goto_payment')) {
          if ($('#frm3').valid()) {
            $self.siblings('.plus').show();
            $self.siblings('h2').removeClass('active ESQ custom_color');
            $self.hide();
            $payment.siblings('h2').addClass('active ESQ custom_color');
            $(".payment .fl_r").show();
            $("#csp2").hide();
            $payment.show();
            if (win_w < 480) {
              $('html,body').animate({'scrollTop':  $payment.offset().top-75}, 200);
            } else {
              $('html,body').animate({'scrollTop':  $payment.offset().top-85}, 200);
            }
          }
        }
      });

      $chooseoffer.click(function() {
        var term = $(this).val();
            $offers.prop("disabled",true);
            $offers.each(function() {
              $e = $(this);
              if ($e.hasClass(term)) {
                $e.prop("disabled",false);
              }
            });

        if ($('input[name=chooseoffer]:checked').val() == '2gifts') {
          $gift1.show();
          $gift2.show();
          $gift2_input.prop('disabled', false);
        } else {
          $gift1.hide();
          $gift2.hide();
          $gift2_input.prop('disabled',true);
        }
      });



    });
  </script>
