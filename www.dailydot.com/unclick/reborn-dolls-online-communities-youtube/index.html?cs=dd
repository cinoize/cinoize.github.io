    <!-- BEGIN: Header Bidding -->
    <script type="text/javascript">
        var adsStart = (new Date()).getTime();
        /* Get domain without subdomain */
        function getDomain(){
            if(document.domain.length){
                var parts = document.domain.replace(/^(www\.)/,"").split('.');
                //is there a subdomain?
                while(parts.length > 2){
                    //removing it from our array
                    var subdomain = parts.shift();
                }
                //getting the remaining 2 elements
                var domain = parts.join('.');
                return domain.replace(/(^\.*)|(\.*$)/g, "");
            }
            return '';
        }
        var post_id = "628226"; // populate with post ID
                var tags = ["dolls","fandom","irl","unclick","youtube"]; // populate with tags
        var categories = ["unclick","irl"]; // populate with categories

        /* Global GPT and PBJS */
        var googletag = googletag || {};
        googletag.cmd = googletag.cmd || [];
        var pbjs = pbjs || {};
        pbjs.que = pbjs.que || [];

        /* Detect Env */
        var device = (window.innerWidth >= 768) ? "desktop" : "mobile";
        var domain = getDomain();
        var subdomain =  window.location.host.split('.')[2] ? window.location.host.split('.')[0].toLowerCase() : "www";

        /* Set Variable to PBJS */
        pbjs.timeStart = adsStart;
        pbjs.post_id = post_id;
        pbjs.tags = tags;
        pbjs.categories = categories;
        pbjs.device = device;
        pbjs.domain = domain;
        pbjs.subdomain = subdomain;

        googletag.cmd.push(function() {
            if(typeof post_id !== "undefined") googletag.cmd.push(function() { googletag.pubads().setTargeting("post_id", post_id.toString()) });
            if(typeof tags !== "undefined") googletag.cmd.push(function() { googletag.pubads().setTargeting("tags", tags) });
            if(typeof categories !== "undefined") googletag.cmd.push(function() { googletag.pubads().setTargeting("categories", categories) });

            // Init DFP
            googletag.pubads().enableSingleRequest();
            //googletag.pubads().collapseEmptyDivs();
            googletag.enableServices();
        });
    </script>
    <script type="text/javascript" async="true" src="https://hb-thedailydot.s3.us-east-2.amazonaws.com/prebid.js"></script>
    <script async='async' src='https://www.googletagservices.com/tag/js/gpt.js'></script>
    <!-- END: Header Bidding -->


    <!-- Start: CREATE AD SPACES -->
    <script>
        document.addEventListener("DOMContentLoaded", function(){
            // https://stackoverflow.com/questions/11381673/detecting-a-mobile-browser
            var env = device; // use already set variable for consistency
            injectInContentAdSpots(env);
            injectFooterAdSpot(env);
			insertDynamiteInline1Ad();
            if (env === 'desktop') {
                injectRightRailAdSpot();
            }
            // Render HB ads
            pbjs.que.push(function() { pbjs.renderAds(); });
        });

        // https://plainjs.com/javascript/manipulation/insert-an-element-after-or-before-another-32/
        function insertAfter(newNode, referenceNode, context = 'none') {
        	if (referenceNode == null && context !== 'none') {
				throw Error('Cannot add in insertAfter because the '+context+' reference did not work');
			} else if (referenceNode == null) {
        		throw Error('Cannot add '+newNode.attributes+' because the reference node did not exist');
			}
            referenceNode.parentNode.insertBefore(newNode, referenceNode.nextSibling);
        }

        function insertBefore(newNode, referenceNode) {
            referenceNode.parentNode.insertBefore(newNode, referenceNode);
        }

        function returnDomNodeForAdDisplayFromID(ID) {
            var blockNode = document.createElement("div");
            blockNode.className = 'dynamite-block';
            var newNode = document.createElement("div");
            newNode.setAttribute('id', ID);
            newNode.dataset.hbad = "true"; // mark div as an adunit for HB
            blockNode.appendChild(newNode);
            return blockNode;
        }

        function injectInContentAdSpots(env) {
            var relevantPTags = document.querySelectorAll('.entry-content > p');
            if (relevantPTags.length < 2) {
                relevantPTags = document.querySelectorAll('.s-post-content > p');
            }
			if (relevantPTags.length < 2) {
				relevantPTags = document.querySelectorAll('.article-content-wrapper > p');
			}
            var totalParagraphs = relevantPTags.length;
            //mobile
            var positions = [
                {distance: .0, id: 'div-gpt-ad-Mobile_content1'},
                {distance: .3, id: 'div-gpt-ad-Mobile_content2'},
                {distance: .5, id: 'div-gpt-ad-Mobile_content3'},
            ];
            if (env === 'desktop') {
                positions = [
                    {distance: .2, id: 'div-gpt-ad-Desktop_content1'},
                    {distance: .7, id: 'div-gpt-ad-Desktop_content2'},
                ];
            }
            var positionIndex = 0;
            var i, paragraph;
            for (i = 0; i < relevantPTags.length; ++i) {
                paragraph = relevantPTags[i];
                var paragraphsWeShouldHaveTraversed = totalParagraphs * positions[positionIndex].distance;
                if (i > paragraphsWeShouldHaveTraversed) {
                    var newNode = returnDomNodeForAdDisplayFromID(positions[positionIndex].id);
                    // insert before the paragraph
                    insertBefore(newNode, paragraph);
                    positionIndex++;
                }
                if (positionIndex >= positions.length) {
                    break;
                }
            }
        }

        function injectFooterAdSpot(env) {
            var positionID = 'div-gpt-ad-Mobile_footer';
            if (env === 'desktop') {
                positionID = 'div-gpt-ad-Desktop_footer';
            }
            var newAdNode = returnDomNodeForAdDisplayFromID(positionID);
            var newDiv = document.createElement('div');
            var btn = document.createElement("button");
            btn.className = 'closer';
            btn.innerHTML = "x";
            btn.onclick = hideCloseable;
			newDiv.className = 'closable';
            // newDiv.appendChild(btn);
			newDiv.appendChild(newAdNode);
            var anchorElement = document.getElementById('page');
            if (anchorElement === null) {
                anchorElement = document.getElementById('page-wrapper');
            }
			if (anchorElement === null) {
				anchorElement = document.getElementById('content');
			}
            insertAfter(newDiv, anchorElement, 'injectFooterAdSpot');
        }
        function hideCloseable() {
            var list = document.getElementsByClassName("closable");
            // conver nodecollection to array
            list = Array.from(list);
            list.forEach(function(item){
                item.style.display = 'none';
            });
        }

        function injectRightRailAdSpot() {
            var newDiv = document.createElement('div');
            newDiv.id = 'stable';
            var anchorElement = document.getElementsByClassName('mini-articles')[0];
            insertAfter(newDiv, anchorElement, 'injectRightRailAdSpot 1st');
            newDiv.appendChild(anchorElement);
            var newNode = returnDomNodeForAdDisplayFromID('div-gpt-ad-Desktop_rr');
            insertAfter(newNode, anchorElement, 'injectRightRailAdSpot 2nd');
        }

        /* Only use our sticky polyfill on the Boombox theme */
        window.addEventListener('DOMContentLoaded', function(event) {
            if (document.body.classList.contains('bb-cards-view')) {
                window.onscroll = function() {boomboxStickyPolyfill()};
            }
        });

        // Add the sticky class to the navbar when you reach its scroll position. Remove "sticky" when you leave the scroll position
        function boomboxStickyPolyfill() {
            // Get the navbar
            var stickyElement = document.getElementById("stable");
            // Get the offset position of the navbar
            var stickyOffset = stickyElement.offsetTop;
            var header = document.getElementsByTagName("header")[0];
            console.log(stickyOffset, window.pageYOffset, header.scrollHeight);
            if (window.pageYOffset >= stickyOffset+header.scrollHeight) {
                stickyElement.classList.add("sticky")
            } else {
                stickyElement.classList.remove("sticky");
            }
        }

        function insertDynamiteInline1Ad() {
			var s = document.createElement('script');
				s.src = "https://live.sekindo.com/live/liveView.php?s=102597&cbuster=[CACHE_BUSTER]&pubUrl=[PAGE_URL_ENCODED]&subId=[SUBID_ENCODED]";
				s.type = "text/javascript";
				s.language = "javascript";
			var relevantPTags = document.querySelectorAll('.entry-content > p');
			if (relevantPTags.length < 2) {
				relevantPTags = document.querySelectorAll('.s-post-content > p');
			}
			if (relevantPTags.length < 2) {
				relevantPTags = document.querySelectorAll('.article-content-wrapper > p');
			}
			var totalParagraphs = relevantPTags.length;
			insertAfter(s, relevantPTags[0]);
		}
    </script>
    <!-- END: CREATE AD SPACES -->
