    <script>
    // The URL in the script tag below is dynamically generated JavaScript that
    // includes the book metadata and page image access functions.
    // The ia{number}.us.archive.org server for the book can and does change,
    // so this URL should NOT be used for permanent access.
    // Use the JSLocate URL below instead for stable access -
    // it will find the item and redirect to the correct server
    // https://archive.org/bookreader/BookReaderJSLocate.php?id=AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnThe&subPrefix=FlorinGeorgeCalian-AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnTheHistoriographyOfAlchemy    </script>

    <script>
      // Load Bookreader data async
      $(function() {
        var paramsToJSON = function paramsToJSON() {
          var params = window.location.search.substring(1);
          var json = {};

          if (params.length > 0) {
            params.split('&').forEach(function splitParams(param) {
              var pair = param.split('=');
              var key = pair[0];
              var value = pair[1];
              json[decodeURIComponent(key)] = decodeURIComponent(value);
            });
          }

          return json;
        };

        $.ajax({
          url: '//ia800301.us.archive.org/BookReader/BookReaderJSIA.php?id=AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnThe&itemPath=/15/items/AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnThe&server=ia800301.us.archive.org&format=jsonp&subPrefix=FlorinGeorgeCalian-AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnTheHistoriographyOfAlchemy&requestUri=/stream/AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnThe/FlorinGeorgeCalian-AlkimiaOperativaAndAlkimiaSpeculativa.SomeModernControversiesOnTheHistoriographyOfAlchemy&version=mHe9koCz',
          type: 'GET',
          dataType: 'jsonp',
        }).then(function(response) {
          BookReaderJSIAinit(response.data);

          // Wire up full-screen button, to bring back to details page
          BookReader.prototype.toggleFullscreen = function() {
            window.location.href = window.location.href.replace('/stream/', '/details/');
          };

          var ResizeBookReader;
          var $IABookReaderWrapper = $('#IABookReaderWrapper');
          var $IABookReaderMessageWrapper = $('#IABookReaderMessageWrapper');
          var $BookReader = $('#BookReader');
          ResizeBookReader = function() {
            $BookReader.css({
              height: $IABookReaderWrapper.innerHeight() - $IABookReaderMessageWrapper.outerHeight()
            });
            br.resize();
          };
          $(window).resize(throttle(function() {
            ResizeBookReader();
          }));
          $(document).bind('IABookReader:ResizeBookReader', ResizeBookReader);

          if (paramsToJSON().wrapper === "false") {
            $("#IABookReaderMessageWrapper").remove();
          }
          ResizeBookReader();
        });
      });

      // Usage stats
      if (window.archive_analytics) {
        window.archive_analytics.values['bookreader'] = 'open'
      };
    </script>
