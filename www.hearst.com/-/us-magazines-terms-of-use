	$(document).ready(function(){
	    var signUp = getUrlParameter('signup');
	    if(signUp == 'hearstlink'){
		   $("#hearstLinkId").click();
		   trackModalHearstLinkExternalCall();
	    }
	});
	
</script>	</header>		
