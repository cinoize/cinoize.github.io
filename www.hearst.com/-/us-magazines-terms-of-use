<script type="text/javascript">
var player_id = "2848955750001";
var player_key = "AQ~~,AAACl1L68lk~,IjWrKCpaAYx7FwXDy0cnc3Sb9gD104Wo";
$(document).ready(function() {	

	if ($(".article-gallery-breaker").length>0) {
      $.each($(".article-gallery-breaker"), function(){
        _breaker_id = $(this).data('breaker-id');
        _article_gallery = $(this);
        _article_gallery_name = $(this).data('breaker-name');
        getArticleGallerySlides(_article_gallery, _article_gallery_name);
      });
    }
    
	function getArticleGallerySlides(_article_gallery, _article_gallery_name) {
		$(".related-article-slideshow-content").each(function(e) {
			var slideshowTitle = $(this).data('title');
			slideshowTitle = slideshowTitle.replace(/[\u00A0\u1680​\u180e\u2000-\u2009\u200a​\u200b​\u202f\u205f​\u3000]/g,'');
			if (slideshowTitle == _article_gallery_name) {
				_article_gallery.replaceWith($(this).html());
			}
		});
		jcarouselBehaviors();
		var jcarousel = $('#article-gallery');
		jcarousel.on('jcarousel:reload jcarousel:create', function () {
			var width = jcarousel.innerWidth();
			width = width / 1;
			jcarousel.jcarousel('items').css('width', width + 'px');
		}).jcarousel();
        jcarousel.data('jcarousel').scroll( 0, false );
	}
	
	function jcarouselBehaviors () {
	    var $jCarousel = $('.article-body-copy .jcarousel'); 
	    $jCarousel.each(function () {
	        
	        var $carousel = $(this);
	        var $wrapper = $carousel.parent();
	        var $prev = $('.jcarousel-control-prev', $wrapper);
	        var $next = $('.jcarousel-control-next', $wrapper);
	        var $pagination = $('.jcarousel-pagination', $wrapper);
	        
	        var jcarouselData = $carousel.jcarousel({ animation: 'fast' });
	        
	        $carousel.on('jcarousel:animateend', function (event, carousel) {
	            var items = carousel.items();
	            var w = 0; 
	            items.each(function () {
	                w += carousel.dimension($(this));
	            });
	            var left = -pxToInt(carousel.list().css('left')) + carousel.clipping();
	            if (left == w) {
	                $('a', $pagination).last().trigger('click');
	            }
	        });
	        
	        $carousel.jcarousel('reload');
	        
	        $prev.on('jcarouselcontrol:active', function () {
	            $(this).removeClass('inactive');
	        }).on('jcarouselcontrol:inactive', function () {
	            $(this).addClass('inactive');
	        }).jcarouselControl({ target: '-=1' });
	        
	        $next.on('jcarouselcontrol:active', function () {
	            $(this).removeClass('inactive');
	        }).on('jcarouselcontrol:inactive', function () {
	            $(this).addClass('inactive');
	        }).jcarouselControl({ target: '+=1' });
	        
	        $pagination.on('jcarouselpagination:active', 'a', function () {
	            $(this).addClass('active');
	        }).on('jcarouselpagination:inactive', 'a', function () {
	            $(this).removeClass('active');
	        }).on('click', function (e) {
	            e.preventDefault();
	        }).jcarouselPagination({ 
	            perPage: _calculatePages 
	        }).data('jcarouselPagination'); 
	        
	        $carousel.touchwipe({ 
	            wipeLeft: function (e) {
	                $carousel.jcarousel('scroll', '+=1'); 
	                e.preventDefault(); 
	            }, wipeRight: function (e) { 
	                $carousel.jcarousel('scroll', '-=1'); 
	                e.preventDefault(); 
	            }, min_move_x: 20, min_move_y: 20, preventDefaultEvents: false 
	        });
	    });
	}
	
	function pxToInt(pxVal) { 
		return typeof pxVal === 'undefined' ? 0 : parseFloat(pxVal.replace('px', '')); 
	};
	        
    function _calculatePages() {
        var carousel = this.carousel().data('jcarousel');
        var items = carousel.items();
        var clip = carousel.clipping();
        var wh = 0;
        var idx = 0;
        var page = 1;
        var curr;
        var count = 0;
        while (true && count < 50) {
            count++; curr = items.eq(idx++); if (curr.length === 0) { break; }
            wh += carousel.dimension(curr); if (wh >= clip) {
                page++; if (wh > clip) { idx--; }
                wh = 0;
            }
        }
        return Math.ceil(items.length / page);
    }
    
    if ($(window).width() < 1296) {
		$(".article-body-copy .slideshow-li").each(function(e){
			$(this).width($(this).parent().parent().width());
		});
    }
    
    $(window).resize(function(e) {
    	if ($(window).width() < 1296) {
    		$(".article-body-copy .slideshow-li").each(function(e){
    			$(this).width($(this).parent().parent().width());
    		});
    	}
    });
    
