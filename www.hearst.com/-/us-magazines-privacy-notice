<script>
$( document ).ready(function() {
	$( "input[type=email]" ).keyup(function(e) {
		var email = $( "input[type=email]" ).val();
		if(!email.match(/^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/)){
    		$("#hearstLinkSubmit").attr('disabled', true);
 	        return false;
 	    } else {
 	    	$(".email-signup-container2 .lfr-status-error-label").hide();
 	    	$("#hearstLinkSubmit").attr('disabled', false);
 	    }
	});
	$( "input[type=email]" ).blur(function(e) {
		var email = $( "input[type=email]" ).val();
		if(!email.match(/^([a-zA-Z0-9_\-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([a-zA-Z0-9\-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/)){
    		$(".email-signup-container2 .lfr-status-error-label").show();
    		return false;
 	    } else {
 	    	$(".email-signup-container2 .lfr-status-error-label").hide();
 	    }
	});
	
    $("#hearstLinkSubmit").click(function(){
    	var email = $( "input[type=email]" ).val();
    	var fname =  $("input[id=firstName]").val();
    	var lname =  $("input[id=lastName]").val();
    	optIn(email, fname, lname);
        $("input[type=email]").val("");
        $("input[id=firstName]").val("");
        $("input[id=lastName]").val("");
    });

    function optIn(email, firstName, lastName) {
	   	Liferay.Service('/hearstsailthru.newsletter/optInExternal', {
			mailId : email,
			firstName : firstName,
			lastName : lastName
		}, function(response) {
			obj = JSON.parse(response);
			var httpCode = obj.httpStatusCode;
			var errorCode = obj.errorCode;
			var responseMessage = obj.responseMessage;
			switch(httpCode) {
				case (200):
					if (obj.jsonNode.lists!=null) {
						console.log("User already subscribed.");
						$("#hearstLinkConfirmationId").show();
						$("#hearstLinkFailureId").hide();
						$("#hearstLinkSignUpFormId").hide();
						clearForm();
					} else if (errorCode!=null && errorCode!="") {
						console.log("Error:" + errorCode + " - " + errorMessage);
						$("#hearstLinkFailureId").show();
						$("#hearstLinkSignUpFormId").hide();
						clearForm();
					} else {
						console.log("User successfuly subscribed!")
						console.log(responseMessage);
						$("#hearstLinkConfirmationId").show();
						$("#hearstLinkFailureId").hide();
						$("#hearstLinkSignUpFormId").hide();
					}
					clearForm();
					break;
				default:
					console.log("Something went wrong with Sailthru: "+ responseMessage + " | View logs.");
					console.log(responseMessage);
					$("#hearstLinkFailureId").show();
					$("#hearstLinkSignUpFormId").hide();
					clearForm();
					break;
			}
        		$("#hearstLinkSubmit").attr('disabled', true);
        		trackHearstLinkSubmitClick();
			}
		);
	}
    
    function clearForm() {
    	$("input[type=email]").val("");
        $("input[id=firstName]").val("");
        $("input[id=lastName]").val("");
        $("#hearstLinkSubmit").attr('disabled', true);
    }
    
    $(".hearstlink-close").click(function(){
    	clearForm();
    	$("#hearstLinkConfirmationId").hide();
		$("#hearstLinkSignUpFormId").show();
    });
});
</script>

	
	
					
				
			
		
	
	


	</div>

