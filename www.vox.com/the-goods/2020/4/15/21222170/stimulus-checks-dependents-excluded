    }
  }
}

window.player_callback = function(unit) {
  unit.init(connatixSettings);
};

(function(){
  // load connatix js
  var connatixLoaded = false;

  function loadConnatix() {
    if (connatixLoaded) {
      return;
    }
    connatixLoaded = true;

    var ref = document.querySelector("div.connatix");
    ref.style.display = 'block';

    var script = document.createElement('script');
    script.setAttribute('type','text/javascript');
    script.setAttribute('src', 'https://cdn.connatix.com/min/connatix.renderer.infeed.min.js');
    script.setAttribute('data-connatix-token', '4cdd477d-4113-4e66-b5a3-4fe56c85c4a6');
    script.setAttribute('ext-interface-callback', 'player_callback');
    script.setAttribute('async', 'async');

    ref.appendChild(script);
  }

  var scrollSubscriber = document.cookie.indexOf("scroll0=") > -1;
  var brandedContent = !!(window.location.href.indexOf(".com/ad/") > -1 || window.location.href.indexOf(".net/ad/") > -1);
  var hymnalPreviewContent = !!(window.location.href.indexOf("concert_preview") > -1);

  if (!scrollSubscriber && !brandedContent && !hymnalPreviewContent) {
    // First, set a timeout to loadConnatix, in case no ads load in 5s
    // (i.e., blocked). Once an ad has loaded, create a new timeout that
    // will load connatix if openmic doesn't get loaded in 2s.
    //
    var adBlockerTimeoutId = setTimeout(loadConnatix, 5000);
    document.body.addEventListener('first_ad_rendered', function(){
      clearTimeout(adBlockerTimeoutId);
      var connatixTimeoutId = setTimeout(loadConnatix, 2000);
      document.body.addEventListener('playlistRendered', function(){
        clearTimeout(connatixTimeoutId);
      });
    });
  }
})();
</script>

      </div>
    
  </div>
