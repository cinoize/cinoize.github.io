(function () {
    var enhancedCSS, my, style;

    if (window.location.search.indexOf('morph-disable-enhanced=1') < 0) {
        my = document.getElementById('nw-c-navigation-enhanced-css-javascript');

        enhancedCSS = document.getElementById('nw-c-navigation-enhanced-css');

        style = document.createElement('style');
        style.type = 'text/css';
        if (style.styleSheet) {
            style.styleSheet.cssText = enhancedCSS.innerHTML;
        } else {
            style.appendChild(document.createTextNode(enhancedCSS.innerHTML));
        }

        my.parentNode.insertBefore(style, my.nextSibling);
    }
})();
