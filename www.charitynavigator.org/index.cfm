<script>
$('.accordion-item-hd').click(function(){
	$('.accordion-item-bd').toggleClass('bordertop');
})
$(function(){
	window.pageFeedback = {
		div$: $("div.pageFeedback")
	};
	pageFeedback.feedbackForm$ = pageFeedback.div$.find("form.feedback");
	pageFeedback.contactForm$ = pageFeedback.div$.find("form.contact-info");
	pageFeedback.questions$ = pageFeedback.feedbackForm$.find("input[type='radio']");
	pageFeedback.content$ = pageFeedback.feedbackForm$.find("textarea.content");

	pageFeedback.identifier$ = pageFeedback.contactForm$.find("input[name='identifier']");
	pageFeedback.email$ = pageFeedback.contactForm$.find("input[name='email']");
	pageFeedback.phone$ = pageFeedback.contactForm$.find("input[name='phone']");

	pageFeedback.feedbackForm$.on("submit", function(event) {
		var this$ = $(this);

		if( pageFeedback.feedbackForm$.hasClass("submitting")  ) {
			return false;
		}

		if( $.trim(pageFeedback.content$.val()) === "" ) {
			var inputs = pageFeedback.questions$.filter(":checked");

			if( inputs.length === 0 ) {
				pageFeedback.feedbackForm$.addClass("problem");

				return false;
			}
		}

		pageFeedback.feedbackForm$.addClass("submitting"); 
		pageFeedback.feedbackForm$.removeClass("error");
		pageFeedback.feedbackForm$.removeClass("problem");

		var options = {
			url: "/beacon/feedback.cfm",
			method: "POST",
			data: this$.serialize(),
			dataType: "json"
		};
		var p = $.ajax(options);
		
		p.then(
			
			function(data) {
				pageFeedback.identifier$.val(data.feedback);

				pageFeedback.div$.addClass("feedback-submitted");
				pageFeedback.feedbackForm$.removeClass("submitting");
				$('.provide-feedback').hide();
				$('.pageFeedback').css('padding', '10px');
			},

			
			function() {
				pageFeedback.feedbackForm$.addClass("error");
				pageFeedback.feedbackForm$.removeClass("submitting");
			}
		);

		return false;
	});

	pageFeedback.contactForm$.on("submit", function(event) {
		var this$ = $(this);

		if( pageFeedback.contactForm$.hasClass("submitting")  ) {
			return false;
		}

		if( $.trim(pageFeedback.email$.val()) === "" ) {
			if( $.trim(pageFeedback.phone$.val()) === "" ) {
				pageFeedback.contactForm$.addClass("problem");

				return false;
			}
		}

		pageFeedback.contactForm$.addClass("submitting"); 
		pageFeedback.contactForm$.removeClass("error");
		pageFeedback.contactForm$.removeClass("problem");

		var options = {
			url: "/beacon/contact.cfm",
			method: "POST",
			data: this$.serialize()
		};
		var p = $.ajax(options);
		
		p.then(
			
			function(data) {
				pageFeedback.contactForm$.addClass("submitted");
				pageFeedback.div$.addClass("contact-submitted");
				pageFeedback.contactForm$.removeClass("submitting");
				setTimeout(function(){$('.elasticsearchfeedback').fadeOut('slow');},3000);
			},

			
			function() {
				pageFeedback.contactForm$.addClass("error");
				pageFeedback.contactForm$.removeClass("submitting");
			}
		);

		return false;
	});
});
