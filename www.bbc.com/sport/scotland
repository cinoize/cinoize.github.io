            function generateCountername() {
                if (window.location.protocol === 'file:') {
                  return 'local.file';
                }
    
                var pathName = window.location.pathname
                    .replace(/\/$/, "")
                    .replace(/^\//, "")
                    .replace(/\//g, '.');
                return pathName + '.page';
            }
    
