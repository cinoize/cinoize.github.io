    srcset="https://cdn.vox-cdn.com/thumbor/yRfTdv3vRlmWy4v-yAmuvsVoqio=/0x0:6720x4480/250x188/filters:focal(2823x1703:3897x2777):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66669851/A46A7505.0.jpg 250w, https://cdn.vox-cdn.com/thumbor/6whFM1AzHCy0G1hRacfimesDzyE=/0x0:6720x4480/500x375/filters:focal(2823x1703:3897x2777):format(webp)/cdn.vox-cdn.com/uploads/chorus_image/image/66669851/A46A7505.0.jpg 500w"
