            <mrow class="MJX-TeXAtom-ORD">
              <mi>i</mi>
            </mrow>
          </munder>
          <msubsup>
            <mi>c</mi>
            <mrow class="MJX-TeXAtom-ORD">
              <mi>i</mi>
            </mrow>
            <mrow class="MJX-TeXAtom-ORD">
              <mi>A</mi>
            </mrow>
