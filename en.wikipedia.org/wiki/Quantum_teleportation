            <mi>A</mi>
          </mrow>
        </msub>
        <mo>,</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mo stretchy="false">|</mo>
        </mrow>
        <msup>
          <mi mathvariant="normal">&#x03A8;<!-- Ψ --></mi>
