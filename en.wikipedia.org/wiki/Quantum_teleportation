            <mi>A</mi>
          </mrow>
        </msub>
        <mo>&#x2297;<!-- ⊗ --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mo stretchy="false">|</mo>
        </mrow>
        <mn>0</mn>
        <msub>
          <mo fence="false" stretchy="false">&#x27E9;<!-- ⟩ --></mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>B</mi>
          </mrow>
        </msub>
        <mo>+</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mo stretchy="false">|</mo>
        </mrow>
        <mn>1</mn>
        <msub>
          <mo fence="false" stretchy="false">&#x27E9;<!-- ⟩ --></mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>A</mi>
          </mrow>
        </msub>
        <mo>&#x2297;<!-- ⊗ --></mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mo stretchy="false">|</mo>
        </mrow>
